<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="606.0" prefWidth="830.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.SupportController">
   <children>
      <Label layoutX="345.0" layoutY="34.0" text="Support">
         <font>
            <Font name="System Bold" size="37.0" />
         </font>
      </Label>
      <ImageView fx:id="supportBackArrow" fitHeight="150.0" fitWidth="45.0" layoutX="35.0" layoutY="528.0" onMouseClicked="#supportBackArrowClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@billeder/backArrow.png" />
         </image>
      </ImageView>
      <TextArea fx:id="issueTextArea" editable="false" layoutX="172.0" layoutY="103.0" prefHeight="374.0" prefWidth="464.0" text="For live support, contact on telephone: 494-585-1882&#10;Open hours &#10;Monday - Friday: 08:00-16:00&#10;Saturday: 10:00-14:00&#10;Sunday: Closed&#10;Otherwise press &quot;next&quot; in order to create a ticket" wrapText="true" />
      <Button fx:id="createTicketButton" layoutX="532.0" layoutY="497.0" mnemonicParsing="false" onAction="#createTicketButtonClicked" text="Next" />
   </children>
</AnchorPane>
