<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.BrugerController">
   <children>
      <Label layoutX="124.0" layoutY="139.0" prefHeight="33.0" prefWidth="135.0" text="Create a new case">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label layoutX="168.0" layoutY="40.0" prefHeight="54.0" prefWidth="465.0" text="Welcome  + Vedkommendes Username ">
         <font>
            <Font size="37.0" />
         </font>
      </Label>
      <Label layoutX="430.0" layoutY="143.0" text="List of cases">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <ListView layoutX="430.0" layoutY="168.0" prefHeight="300.0" prefWidth="275.0" />
      <ImageView fx:id="createCaseImage" fitHeight="150.0" fitWidth="200.0" layoutX="109.0" layoutY="200.0" onMouseClicked="#createCaseImageClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@billeder/caseCustomer.png" />
