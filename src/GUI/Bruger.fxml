      </ImageView>
      <ImageView fx:id="helpImage" fitHeight="150.0" fitWidth="50.0" layoutX="14.0" layoutY="486.0" onMouseClicked="#helpImageClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@billeder/support.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
