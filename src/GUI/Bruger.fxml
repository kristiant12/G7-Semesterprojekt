      </ImageView>
      <ImageView fx:id="logOutImage" fitHeight="150.0" fitWidth="50.0" layoutX="376.0" layoutY="486.0" onMouseClicked="#logOutImageClicked" pickOnBounds="true" preserveRatio="true">
