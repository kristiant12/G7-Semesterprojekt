<AnchorPane id="AnchorPane" fx:id="browsePicturesButton" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.UploadPicturesController">
   <children>
      <Label layoutX="168.0" layoutY="42.0" text="Upload Pictures">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <TextField fx:id="uploadPicsTextfield" layoutX="168.0" layoutY="187.0" prefHeight="25.0" prefWidth="312.0" />
      <Button fx:id="browsePicturesButton" layoutX="420.0" layoutY="187.0" mnemonicParsing="false" onAction="#browsePicturesButtonClick" prefHeight="25.0" prefWidth="60.0" text="Browse.." />
      <Button fx:id="uploadButton" layoutX="274.0" layoutY="310.0" mnemonicParsing="false" onAction="#uploadButtonClick" text="Upload" />
   </children>
</AnchorPane>
